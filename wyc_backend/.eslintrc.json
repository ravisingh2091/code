{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "brace-style": "off",
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "curly": "error",
    "eol-last": [
      "error",
      "always"
    ],
    "eqeqeq": "error",
    "handle-callback-err": "error",
    "indent": 2,
    "keyword-spacing": [
      2,
      {
        "after": true
      }
    ],
    "no-cond-assign": "error",
    "no-console":"off",
    "no-const-assign": 2,
    "no-dupe-keys": 2,
    "no-duplicate-imports": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-multiple-empty-lines": "error",
    "no-multi-spaces": "error",
    "no-undef": "error",
    "no-multi-str": "error",
    "no-redeclare": "error",
    "no-return-assign": "error",
    "no-self-assign": "error",
    "no-self-compare": "error",
    "no-sparse-arrays": "error",
    "no-tabs": "error",
    "no-undef-init": "error",
    "no-unmodified-loop-condition": "error",
    "no-unneeded-ternary": "error",
    "no-unreachable":"off",
    "no-unused-vars": "error",
    "no-useless-computed-key": "error",
    "no-useless-rename": "error",
    "no-useless-escape": "error",
    "no-var": 2,
    "no-whitespace-before-property": "error",
    "operator-linebreak": [
      "error",
      "after"
    ],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "padded-blocks": [
      "error",
      "never"
    ],
    "prefer-const": [
      2,
      {
        "destructuring": "all"
      }
    ],
    "rest-spread-spacing": "error",
    "semi": [
      "error",
      "always"
    ],
    "semi-spacing": "error",
    "space-before-blocks": "error",
    "space-in-parens": [
      "error",
      "never"
    ],
    "space-infix-ops": "error",
    "space-unary-ops": "error",
    "spaced-comment": "error",
    "use-isnan": "error",
    "template-curly-spacing": "error",
    "valid-typeof": "error",
    "wrap-iife": [
      "error",
      "outside"
    ],
    "yield-star-spacing": [
      "error",
      "after"
    ],
    "yoda": 2
  }
}