{
  "name": "petparenting",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cluster.js",
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.9.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "pm2 restart dist/cluster.js",
    "poststart": "pm2 log 0",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir -p dist && babel src -s -D -d dist",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-fileupload": "^1.1.6",
    "express-validator": "^5.3.0",
    "firebase-admin": "^8.12.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.22.2",
    "mongoose": "^5.2.15",
    "morgan": "^1.9.0",
    "nodemailer": "^6.4.2",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^25.0.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-watch": "^3.1.3",
    "jest": "^25.0.0",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  },
  "author": "",
  "license": "MIT"
}
